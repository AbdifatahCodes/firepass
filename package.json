{
  "name": "firepass",
  "displayName": "Firepass",
  "version": "0.0.1",
  "description": "An Extension for Firepass Browsing Manager",
  "author": "AbdifatahCodes <abdifatahosman.dev@gmail.com>",
  "scripts": {
    "dev": "plasmo dev --target=firefox-mv3",
    "debug": "plasmo dev --verbose --target=firefox-mv3",
    "build": "plasmo build --target=firefox-mv3",
    "package": "plasmo package"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "graphql": "^16.8.1",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage",
      "tabs"
    ]
  }
}
